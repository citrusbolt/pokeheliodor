gengenrtl.exe: gengenrtl.obj,obstack.obj,$(ALLOCA)
	$(LINK) $(LFLAGS)/exe=$@ $^$(LIBS)

genrtl.h genrtl.c: gengenrtl.exe
	mcr $(PWD)gengenrtl.exe	genrtl.h genrtl.c

