cleancccp:
	$$ purge
	$(RM) cccp.obj;,cexp.obj;
	$(RM) cpp.exe;

cleanlib:
	$$ purge
	$(RM) libgcc2.olb;

cleanlibplus:
	$$ purge
	$(RM) [.cp]tinfo.obj;
	$(RM) [.cp]tinfo2.obj;
	$(RM) [.cp]new.obj;
	$(RM) [.cp]new1.obj;
	$(RM) [.cp]new2.obj;
	$(RM) [.cp]exception.obj;
	$(RM) libgccplus.olb;

clean:
	$$ purge
	$$ purge [.cp]
	$(RM) *.obj;*
	$(RM) [.cp]*.obj;*
	$(RM) [.cp]parse.output;*
	$(RM) *.cpp;*
	$(RM) *.s;*
	$(RM) *.rtl;*
	$(RM) a.out;
	$(RM) *.combine;
	$(RM) *.cpp;
	$(RM) *.cse;
	$(RM) *.cse2;
	$(RM) *.dbr;
	$(RM) *.flow;
	$(RM) *.greg;
	$(RM) *.jump;
	$(RM) *.jump2;
	$(RM) *.loop;
	$(RM) *.lreg;
	$(RM) *.rtl;
	$(RM) *.sched;
	$(RM) *.sched2;
	$(RM) *.map;
	$(RM) genattr.exe;,insn-attr.h;
	$(RM) genflags.exe;,insn-flags.h;
	$(RM) gencodes.exe;,insn-codes.h;
	$(RM) genconfig.exe;,insn-config.h;
	$(RM) genpeep.exe;,insn-peep.c;
