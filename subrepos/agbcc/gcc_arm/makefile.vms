genattrtab.exe: genattrtab.obj,rtlanal.obj,$(GENOBJS)$(ALLOCA)
	$(LINK) $(LFLAGS)/exe=$@ $^$(LIBS)

insn-attrtab.c: genattrtab.exe $(MDFILE)
